{
  "name": "base-symphony-js-api-sql",
  "version": "1.0.0",
  "description": "NodeJS base project with TypeScript for the rapid creation of backends with certain functionalities already created and programming patterns already defined.",
  "author": {
    "name": "Luis Fernando Solano Mart√≠nez",
    "email": "luisfer.sm15@gmail.com",
    "url": "https://github.com/XxLuisFer15xX"
  },
  "private": true,
  "license": "MIT",
  "keywords": [
    "screaming architecture",
    "node",
    "nodejs",
    "javascript",
    "typescript",
    "express",
    "nestjs",
    "swagger",
    "prisma",
    "sqlserver",
    "jsonwebtoken",
    "eslint",
    "prettier",
    "path alias",
    "testing",
    "mit"
  ],
  "scripts": {
    "build": "nest build",
    "postbuild": "cpx src/**/*.html dist/src/",
    "postinstall": "prisma generate",
    "prepare": "husky install",
    "format": "prettier --write \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "format-check": "npx prettier --check .",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,__tests__}/**/*.ts\" --max-warnings 0",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest-e2e.config.ts",
    "generate-credentials:local": "sh generate-environment-credentials.sh local",
    "generate-credentials:dev": "sh generate-environment-credentials.sh dev",
    "generate-credentials:qa": "sh generate-environment-credentials.sh qa",
    "generate-credentials:prod": "sh generate-environment-credentials.sh prod"
  },
  "dependencies": {
    "@nestjs/common": "10.0.0",
    "@nestjs/core": "10.0.0",
    "@nestjs/platform-express": "10.0.0",
    "@nestjs/swagger": "8.1.0",
    "@prisma/client": "6.1.0",
    "bcrypt": "5.1.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "cloudinary": "2.5.1",
    "dotenv": "16.4.7",
    "google-auth-library": "9.15.0",
    "joi": "17.13.3",
    "jsonwebtoken": "9.0.2",
    "morgan": "1.10.0",
    "nodemailer": "6.9.16",
    "reflect-metadata": "0.2.0",
    "rxjs": "7.8.1",
    "swagger-ui-express": "5.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.6.1",
    "@commitlint/config-conventional": "19.6.0",
    "@nestjs/cli": "10.0.0",
    "@nestjs/schematics": "10.0.0",
    "@nestjs/testing": "10.0.0",
    "@types/bcrypt": "5.0.2",
    "@types/express": "5.0.0",
    "@types/jest": "29.5.2",
    "@types/morgan": "1.9.9",
    "@types/node": "20.3.1",
    "@types/nodemailer": "6.4.17",
    "@types/supertest": "6.0.0",
    "@typescript-eslint/eslint-plugin": "8.0.0",
    "@typescript-eslint/parser": "8.0.0",
    "cpx": "1.5.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "husky": "9.1.7",
    "jest": "29.5.0",
    "prettier": "3.0.0",
    "prisma": "6.1.0",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.3"
  }
}
